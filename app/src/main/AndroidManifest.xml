<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mozdevz.org.gdgmaputo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name="mozdevz.org.devfestmaputo.Activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="mozdevz.org.devfestmaputo.Activities.TeamActivity"
            android:label="@string/title_activity_team"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="mozdevz.org.devfestmaputo.Activities.SessionActivity"
            android:label="@string/title_activity_session"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="mozdevz.org.devfestmaputo.Activities.SpeakerActivity"
            android:label="@string/title_activity_speaker"></activity>
    </application>

</manifest>